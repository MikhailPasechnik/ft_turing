            {"read": "B", "to_state": "success", "write": "B", "action": "RIGHT"}
        ],
        "return": [
            {"read": "a", "to_state": "return", "write": "a", "action": "LEFT"},
            {"read": "b", "to_state": "return", "write": "b", "action": "LEFT"},
            {"read": "A", "to_state": "on_left", "write": "A", "action": "RIGHT"},
            {"read": "B", "to_state": "on_left", "write": "B", "action": "RIGHT"}
        ],
        "success": [
            {"read": "a", "to_state": "success", "write": "A", "action": "RIGHT"},
            {"read": "b", "to_state": "success", "write": "B", "action": "RIGHT"},
            {"read": "A", "to_state": "success", "write": "A", "action": "RIGHT"},
            {"read": "B", "to_state": "success", "write": "B", "action": "RIGHT"},
